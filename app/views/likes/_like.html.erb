<div class="iine">
<h3>いいね件数: <%= plan.likes.count %></h3>
  <% if current_user.already_liked?(plan) %>
    <%= button_to 'いいねを取り消す', plan_likes_path(plan), method: :delete, remote: true %>
  <% else %>
    <%= button_to 'いいね', plan_likes_path(plan), method: :post, remote: true %>
  <% end %>
 </div>